package ex3;

public class UpcastingTest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
//		첫 번째
//		Person p;
//		Student s = new Student("이재문");
		
//		두 번째
		Person p = new Student("이재문"); // 다형성
		Student s;
		
		s = (Student)p; // 다운 캐스팅
		
//		첫 번째
//		p = s; // 업캐스팅(위로 형변환) 다형성
		
//		첫 번째
//		System.out.println(p.name);
		
//		두 번째
		System.out.println(s.name);
		s.grade = "A";
	}

}
