package ex2;

public abstract class Animal {
//	추상 메소드
//	1번 클래스 : 출력 = "야옹~ 야옹~"
//	2번 클래스 : 출력 = "댕!~ 댕!"
	abstract void play();
	
//	1. 오버라이딩 할 메소드 : 출력 = "야옹~ 야옹~ 꼬리 살랑 살랑"
//	2. 오버라이딩 할 메소드 : 출력 = "댕~ 댕~ 꼬리 살랑 살랑"
	void runSomething() {
		System.out.println("꼬리 살랑 살랑~");
	}
	
//	템플릿 메소드
	public void playWithOwner() {
		System.out.println("커염둥이 이리온~");
		play();
		runSomething();
		System.out.println("굿 좌압~");
	}
}
